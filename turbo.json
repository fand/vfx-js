{
    "$schema": "https://turborepo.org/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build",
                "^typedoc"
            ],
            "outputs": [
                "lib",
                "build",
                "dist"
            ]
        },
        "dev": {
            "outputs": [],
            "persistent": true,
            "cache": false
        },
        "clean": {
            "outputs": []
        },
        "lint": {
            "outputs": []
        },
        "test": {
            "outputs": []
        },
        "deploy": {
            "dependsOn": [
                "build",
                "test",
                "lint"
            ],
            "outputs": []
        },
        "typedoc": {}
    }
}
